function IncludeJavaScript(jsFile) {
    document.write('<script type="text/javascript" src="' + jsFile + '"></scr' + 'ipt>');
}

function IncludeCSS(csFile) {
    document.write('<link href="' + csFile + '" rel="stylesheet" type="text/css" media="all" />');
}

